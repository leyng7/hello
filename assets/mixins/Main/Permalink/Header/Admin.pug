mixin admin()
  a.modify(href='[##_s_ad_m_link_##]') 수정
  button.delete(onclick='[##_s_ad_d_onclick_##]') 삭제
  button.visibility(onclick='[##_s_ad_s2_onclick_##]') [##_s_ad_s1_label_##]

span#admin(class='flex gap-x-1')
  if process.env.NODE_ENV === 'development'
    if process.env.IS_OWNER === 'true' && process.env.LOGIN === 'true'
      +admin()
  else
    s_ad_div
      +admin()
